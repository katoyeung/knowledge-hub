# Performance Optimization Settings
# Enable worker pool for CPU-intensive tasks
EMBEDDING_WORKER_POOL_ENABLED=true
EMBEDDING_WORKER_COUNT=3
EMBEDDING_MAX_QUEUE_SIZE=1000
EMBEDDING_WORKER_TIMEOUT=300000

# Queue settings - more balanced
QUEUE_CONCURRENCY=2
QUEUE_MAX_ATTEMPTS=3
QUEUE_BACKOFF_DELAY=2000
QUEUE_REMOVE_ON_COMPLETE=500
QUEUE_REMOVE_ON_FAIL=100

# CPU throttling - less aggressive
HTTP_REQUEST_PRIORITY=false
MAX_CONCURRENT_JOBS=4

# Embedding batch settings
EMBEDDING_BATCH_SIZE=2
EMBEDDING_MAX_CONCURRENCY=3